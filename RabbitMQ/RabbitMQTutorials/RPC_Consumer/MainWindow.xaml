<Window x:Class="RPC_Consumer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RPC_Consumer"
        mc:Ignorable="d"
        Title="CONSUMER Server - RPC" Height="450" Width="500"
        Closing="Window_Closing">
    <Grid>
        <Button x:Name="btnStartReceiving" Content="Start Receiving Message" HorizontalAlignment="Left" Margin="33,28,0,0" VerticalAlignment="Top" Click="btnStartReceiving_Click"/>
        <TextBox x:Name="txtLog" HorizontalAlignment="Center" Margin="0,78,0,0" TextWrapping="Wrap" AcceptsReturn="True" VerticalScrollBarVisibility="Visible" VerticalAlignment="Top" Width="450" Height="250"/>
    </Grid>
</Window>
